{
  "compilerOptions": {
    "target": "ES2015",
    "lib": ["ES2015"],
    "module": "commonjs",
    "strict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "inlineSourceMap": true
  }
}
